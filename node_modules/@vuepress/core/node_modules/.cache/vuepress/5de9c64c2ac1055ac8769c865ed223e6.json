},\n    destroy() {\n      if (screenfull.enabled) {\n        screenfull.off('change', this.change)\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"stylus\" scoped>\n.fullscreen-wrapper {\n  margin-right: 1em;\n  .iconfont {\n    font-size 1.4rem\n    color: $accentColor\n  }\n}\n</style>\n"]}]}